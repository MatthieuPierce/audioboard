ch) => {\n  return {\n    fetchPads: () => dispatch(fetchPads())\n  }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(DrumMachine);","/home/matthieu/Downloads/coding/audioboard/src/redux/actions.js",[],"/home/matthieu/Downloads/coding/audioboard/src/components/Pad.js",[],"/home/matthieu/Downloads/coding/audioboard/src/redux/constants.js",[],["47","48"],"/home/matthieu/Downloads/coding/audioboard/src/redux/rootReducer.js",[],{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":1,"column":8,"nodeType":"55","messageId":"56","endLine":1,"endColumn":12},{"ruleId":"53","severity":1,"message":"57","line":4,"column":8,"nodeType":"55","messageId":"56","endLine":4,"endColumn":20},{"ruleId":"58","severity":1,"message":"59","line":9,"column":3,"nodeType":"60","messageId":"61","endLine":12,"endColumn":4},{"ruleId":"49","replacedBy":"62"},{"ruleId":"51","replacedBy":"63"},"no-native-reassign",["64"],"no-negated-in-lhs",["65"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'ReactFCCtest' is defined but never used.","no-useless-constructor","Useless constructor.","MethodDefinition","noUselessConstructor",["64"],["65"],"no-global-assign","no-unsafe-negation"]